<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="web.scheduler.dao.BookCheckDao">
	
	<update id="update" parameterType="int">
		UPDATE booking
		<set>
			book_status = '0'
		</set>
		<where>
			book_status = '1'
		</where>		
	</update>

</mapper>